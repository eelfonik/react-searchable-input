    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-test-renderer": "^16.8.4",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "styled-components": "^4.1.3"
  },
  "peerDependencies": {
    "styled-components": ">= 3.3.0",
    "react": ">= 16",
    "react-dom": ">= 16",
    "lodash": ">= 4"
